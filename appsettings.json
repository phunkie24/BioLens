{
  "Gemini": {
    "ApiKey": "YOUR_GEMINI_3_API_KEY_HERE",
    "Model": "gemini-3-pro",
    "MaxTokens": 4096,
    "Temperature": 0.2,
    "TopP": 0.95,
    "TopK": 40
  },
  "Database": {
    "ConnectionString": "Data Source=biolens.db",
    "EnableSensitiveDataLogging": false
  },
  "OfflineMode": {
    "Enabled": true,
    "CacheCommonConditions": true,
    "MaxCacheSize": 100,
    "CacheExpirationHours": 168
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "BioLens.Agents": "Debug"
    }
  },
  "Sync": {
    "IntervalSeconds": 300,
    "BatchSize": 50
  }
}
